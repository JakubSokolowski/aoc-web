{
    "version": 2,
    "projects": {
        "aoc-web-front": {
            "root": "apps/aoc-web-front",
            "sourceRoot": "apps/aoc-web-front/src",
            "projectType": "application",
            "targets": {
                "build": {
                    "executor": "@nrwl/web:build",
                    "outputs": ["{options.outputPath}"],
                    "options": {
                        "outputPath": "dist/apps/aoc-web-front",
                        "index": "apps/aoc-web-front/src/index.html",
                        "main": "apps/aoc-web-front/src/main.tsx",
                        "polyfills": "apps/aoc-web-front/src/polyfills.ts",
                        "tsConfig": "apps/aoc-web-front/tsconfig.app.json",
                        "assets": [
                            "apps/aoc-web-front/src/favicon.ico",
                            "apps/aoc-web-front/src/assets"
                        ],
                        "styles": ["apps/aoc-web-front/src/styles.scss"],
                        "scripts": [],
                        "webpackConfig": "@nrwl/react/plugins/webpack"
                    },
                    "dependsOn": [
                        {
                            "target": "build",
                            "projects": "dependencies"
                        }
                    ],
                    "configurations": {
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "apps/aoc-web-front/src/environments/environment.ts",
                                    "with": "apps/aoc-web-front/src/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "500kb",
                                    "maximumError": "1mb"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "executor": "@nrwl/web:dev-server",
                    "options": {
                        "buildTarget": "aoc-web-front:build",
                        "hmr": true
                    },
                    "dependsOn": [
                        {
                            "target": "build",
                            "projects": "dependencies"
                        }
                    ],
                    "configurations": {
                        "production": {
                            "buildTarget": "aoc-web-front:build:production",
                            "hmr": false
                        }
                    }
                },
                "lint": {
                    "executor": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": [
                            "apps/aoc-web-front/**/*.{ts,tsx,js,jsx}"
                        ]
                    }
                },
                "test": {
                    "executor": "@nrwl/jest:jest",
                    "outputs": ["coverage/apps/aoc-web-front"],
                    "options": {
                        "jestConfig": "apps/aoc-web-front/jest.config.js",
                        "passWithNoTests": true
                    }
                }
            }
        },
        "aoc-web-front-e2e": {
            "root": "apps/aoc-web-front-e2e",
            "sourceRoot": "apps/aoc-web-front-e2e/src",
            "projectType": "application",
            "targets": {
                "e2e": {
                    "executor": "@nrwl/cypress:cypress",
                    "options": {
                        "cypressConfig": "apps/aoc-web-front-e2e/cypress.json",
                        "tsConfig": "apps/aoc-web-front-e2e/tsconfig.e2e.json",
                        "devServerTarget": "aoc-web-front:serve"
                    },
                    "dependsOn": [
                        {
                            "target": "build",
                            "projects": "dependencies"
                        }
                    ],
                    "configurations": {
                        "production": {
                            "devServerTarget": "aoc-web-front:serve:production"
                        }
                    }
                },
                "lint": {
                    "executor": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": [
                            "apps/aoc-web-front-e2e/**/*.{js,ts}"
                        ]
                    }
                }
            }
        },
        "lib-rs": {
            "root": "libs/lib-rs",
            "sourceRoot": "libs/lib-rs/src",
            "projectType": "library",
            "targets": {
                "lint": {
                    "executor": "@nrwl/workspace:run-commands",
                    "options": {
                        "commands": [
                            {
                                "command": "cargo clippy -- -D warnings"
                            }
                        ],
                        "cwd": "libs/lib-rs"
                    }
                },
                "format": {
                    "executor": "@nrwl/workspace:run-commands",
                    "options": {
                        "commands": [
                            {
                                "command": "cargo fmt"
                            }
                        ],
                        "cwd": "libs/lib-rs"
                    }
                },
                "test": {
                    "executor": "@nrwl/workspace:run-commands",
                    "options": {
                        "commands": [
                            {
                                "command": "cargo build"
                            },
                            {
                                "command": "cargo test"
                            },
                            {
                                "command": "wasm-pack test --headless --firefox"
                            }
                        ],
                        "cwd": "libs/lib-rs",
                        "parallel": false
                    }
                },
                "build": {
                    "executor": "@nrwl/workspace:run-commands",
                    "options": {
                        "commands": [
                            {
                                "command": "wasm-pack build"
                            }
                        ],
                        "cwd": "libs/lib-rs"
                    }
                }
            }
        }
    },
    "cli": {
        "defaultCollection": "@nrwl/react"
    },
    "generators": {
        "@nrwl/react": {
            "application": {
                "style": "scss",
                "linter": "eslint",
                "strict": true,
                "babel": true
            },
            "component": {
                "style": "scss"
            },
            "library": {
                "style": "scss",
                "linter": "eslint",
                "strict": true
            }
        }
    },
    "defaultProject": "aoc-web-front"
}
